{
    "kind": "collectionType",
    "collectionName": "filings",
    "info": {
        "singularName": "filing",
        "pluralName": "filings",
        "displayName": "Filing",
        "description": "Main filing orchestrator table"
    },
    "options": {
        "draftAndPublish": false
    },
    "pluginOptions": {},
    "attributes": {
        "user": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "plugin::users-permissions.user",
            "inversedBy": "filings"
        },
        "taxYear": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::tax-year.tax-year",
            "inversedBy": "filings"
        },
        "filingType": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::filing-type.filing-type",
            "inversedBy": "filings"
        },
        "filingStatus": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::filing-status.filing-status",
            "inversedBy": "filings"
        },
        "payments": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::payment.payment",
            "mappedBy": "filing"
        },
        "latestFeeSnapshot": {
            "type": "json"
        },
        "entityName": {
            "type": "string"
        },
        "confirmationNumber": {
            "type": "string",
            "unique": true
        },
        "estimatedRefund": {
            "type": "decimal"
        },
        "totalPrice": {
            "type": "decimal",
            "default": 0,
            "description": "Calculated total price for the filing (including tax)"
        },
        "paidAmount": {
            "type": "decimal",
            "default": 0,
            "description": "Amount already paid by user (used for amendment tracking)"
        },
        "uploadedDocuments": {
            "type": "media",
            "multiple": true,
            "required": false,
            "allowedTypes": [
                "files",
                "images"
            ]
        },
        "progress": {
            "type": "integer",
            "default": 0,
            "min": 0,
            "max": 100
        },
        "submittedAt": {
            "type": "datetime"
        },
        "reviewedAt": {
            "type": "datetime"
        },
        "completedAt": {
            "type": "datetime"
        },
        "rejectedAt": {
            "type": "datetime"
        },
        "rejectionReason": {
            "type": "text"
        },
        "assignedTo": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "plugin::users-permissions.user"
        },
        "internalNotes": {
            "type": "richtext"
        },
        "lastStatusChangeAt": {
            "type": "datetime"
        },
        "lastStatusChangeBy": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "plugin::users-permissions.user"
        },
        "personalFilings": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::personal-filing.personal-filing",
            "mappedBy": "filing"
        },
        "corporateFiling": {
            "type": "relation",
            "relation": "oneToOne",
            "target": "api::corporate-filing.corporate-filing",
            "inversedBy": "filing"
        },
        "trustFiling": {
            "type": "relation",
            "relation": "oneToOne",
            "target": "api::trust-filing.trust-filing",
            "inversedBy": "filing"
        },
        "filingData": {
            "type": "json"
        },
        "wizardProgress": {
            "type": "json",
            "description": "Stores wizard progress for resume functionality (lastPhase, lastSectionIndex, lastPersonalFilingId, lastDependentIndex)"
        }
    }
}