{
    "kind": "collectionType",
    "collectionName": "filings",
    "info": {
        "singularName": "filing",
        "pluralName": "filings",
        "displayName": "Filing",
        "description": "Main filing orchestrator table"
    },
    "options": {
        "draftAndPublish": false
    },
    "pluginOptions": {},
    "attributes": {
        "user": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "plugin::users-permissions.user",
            "inversedBy": "filings"
        },
        "taxYear": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::tax-year.tax-year",
            "inversedBy": "filings"
        },
        "filingType": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::filing-type.filing-type",
            "inversedBy": "filings"
        },
        "status": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::filing-status.filing-status",
            "inversedBy": "filings"
        },
        "payment": {
            "type": "relation",
            "relation": "oneToOne",
            "target": "api::payment.payment",
            "mappedBy": "filing"
        },
        "entityName": {
            "type": "string",
            "description": "Name of the person, company, or trust"
        },
        "confirmationNumber": {
            "type": "string",
            "unique": true
        },
        "estimatedRefund": {
            "type": "decimal"
        },
        "uploadedDocuments": {
            "type": "media",
            "multiple": true,
            "required": false,
            "allowedTypes": [
                "files",
                "images"
            ]
        },
        "progress": {
            "type": "integer",
            "default": 0,
            "min": 0,
            "max": 100
        },
        "submittedAt": {
            "type": "datetime"
        },
        "reviewedAt": {
            "type": "datetime"
        },
        "completedAt": {
            "type": "datetime"
        },
        "rejectedAt": {
            "type": "datetime"
        },
        "rejectionReason": {
            "type": "text"
        },
        "assignedTo": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "plugin::users-permissions.user"
        },
        "internalNotes": {
            "type": "richtext"
        },
        "lastStatusChangeAt": {
            "type": "datetime"
        },
        "lastStatusChangeBy": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "plugin::users-permissions.user"
        }
    }
}