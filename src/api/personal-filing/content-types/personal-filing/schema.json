{
  "kind": "collectionType",
  "collectionName": "personal_filings",
  "info": {
    "singularName": "personal-filing",
    "pluralName": "personal-filings",
    "displayName": "Personal Filing (T1)",
    "description": "Personal tax filing data for T1 returns"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "filing": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::filing.filing",
      "inversedBy": "personalFilings"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "primary",
        "spouse",
        "dependent"
      ],
      "default": "primary"
    },
    "individualStatus": {
      "type": "enumeration",
      "enum": [
        "DRAFT",
        "COMPLETED",
        "FLAGGED",
        "VERIFIED"
      ],
      "default": "DRAFT"
    },
    "documents": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "files",
        "images"
      ]
    },
    "firstName": {
      "type": "string"
    },
    "middleName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "sin": {
      "type": "string",
      "private": true
    },
    "dateOfBirth": {
      "type": "date"
    },
    "email": {
      "type": "email"
    },
    "phoneNumber": {
      "type": "string"
    },
    "streetNumber": {
      "type": "string"
    },
    "streetName": {
      "type": "string"
    },
    "apartmentNumber": {
      "type": "string"
    },
    "city": {
      "type": "string"
    },
    "province": {
      "type": "string"
    },
    "postalCode": {
      "type": "string"
    },
    "statusInCanada": {
      "type": "enumeration",
      "enum": [
        "CANADIAN_CITIZEN",
        "PERMANENT_RESIDENT",
        "TEMPORARY_RESIDENT",
        "PROTECTED_PERSON"
      ]
    },
    "dateBecameResident": {
      "type": "date"
    },
    "provinceResided": {
      "type": "string"
    },
    "livedOutsideCanada": {
      "type": "enumeration",
      "enum": [
        "YES",
        "NO"
      ]
    },
    "countryOfResidence": {
      "type": "string"
    },
    "becameResidentThisYear": {
      "type": "enumeration",
      "enum": [
        "YES",
        "NO"
      ]
    },
    "worldIncome": {
      "type": "decimal"
    },
    "maritalStatus": {
      "type": "enumeration",
      "enum": [
        "SINGLE",
        "MARRIED",
        "COMMON_LAW",
        "SEPARATED",
        "DIVORCED",
        "WIDOWED"
      ]
    },
    "maritalStatusChanged": {
      "type": "enumeration",
      "enum": [
        "Yes",
        "No"
      ]
    },
    "maritalStatusChangeDate": {
      "type": "date"
    },
    "spouse": {
      "type": "component",
      "component": "filing.spouse-info",
      "repeatable": false
    },
    "dependents": {
      "type": "component",
      "component": "filing.dependent-info",
      "repeatable": true
    },
    "dependentsCount": {
      "type": "integer",
      "default": 0
    },
    "hasFamilyMembers": {
      "type": "enumeration",
      "enum": [
        "SPOUSE",
        "DEPENDENTS",
        "BOTH",
        "NONE"
      ]
    },
    "incomeSources": {
      "type": "json"
    },
    "deductionSources": {
      "type": "json"
    },
    "taxSlips": {
      "type": "json"
    },
    "additionalDocs": {
      "type": "json"
    },
    "electionsCanada": {
      "type": "component",
      "component": "filing.elections-canada",
      "repeatable": false
    },
    "propertyAssets": {
      "type": "component",
      "component": "filing.property-assets",
      "repeatable": false
    },
    "disabilityCredit": {
      "type": "component",
      "component": "filing.disability-credit",
      "repeatable": false
    },
    "workExpenses": {
      "type": "component",
      "component": "filing.work-expenses",
      "repeatable": false
    },
    "homeOffice": {
      "type": "component",
      "component": "filing.home-office",
      "repeatable": false
    },
    "vehicleExpenses": {
      "type": "component",
      "component": "filing.vehicle-expenses",
      "repeatable": false
    },
    "selfEmployment": {
      "type": "component",
      "component": "filing.self-employment",
      "repeatable": false
    },
    "rentalIncome": {
      "type": "component",
      "component": "filing.rental-income",
      "repeatable": false
    },
    "movingExpenses": {
      "type": "component",
      "component": "filing.moving-expenses",
      "repeatable": false
    },
    "formData": {
      "type": "json"
    },
    "additionalNotes": {
      "type": "text"
    }
  }
}