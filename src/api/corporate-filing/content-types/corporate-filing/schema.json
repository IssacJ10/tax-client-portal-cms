{
  "kind": "collectionType",
  "collectionName": "corporate_filings",
  "info": {
    "singularName": "corporate-filing",
    "pluralName": "corporate-filings",
    "displayName": "Corporate Filing (T2)",
    "description": "Corporate tax filing data for T2 returns"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "filing": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::filing.filing",
      "mappedBy": "corporateFiling"
    },
    "legalName": {
      "type": "string",
      "required": false
    },
    "businessNumber": {
      "type": "string",
      "required": false
    },
    "address": {
      "type": "text"
    },
    "incorporationDate": {
      "type": "date"
    },
    "fiscalYearEnd": {
      "type": "date"
    },
    "shareholders": {
      "type": "json"
    },
    "totalRevenue": {
      "type": "decimal"
    },
    "netIncome": {
      "type": "decimal"
    },
    "expenses": {
      "type": "json"
    },
    "financialStatements": {
      "type": "json"
    },
    "formData": {
      "type": "json"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "DRAFT",
        "COMPLETED",
        "FLAGGED",
        "VERIFIED"
      ],
      "default": "DRAFT"
    }
  }
}